@model IEnumerable<EntryDesignConcept.Models.User>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Fullname)
        </th>       
        <th>
            Position
        </th>
        <th>BU</th>
        <th>Team</th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Fullname)
        </td>      
        <td>
            @Html.DisplayFor(modelItem => item.PositionID.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrganizationID.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TeamID.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID })            
        </td>
    </tr>
}

</table>
